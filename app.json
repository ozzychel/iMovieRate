{
  "expo": {
    "name": "iMovieRate",
    "slug": "iMovieRate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1f1e1e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidNavigationBar": {
      "visible": "sticky",
      "barStyle": "dark-content",
      "backgroundColor": "#1f1e1e"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
